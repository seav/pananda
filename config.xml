<?xml version='1.0' encoding='utf-8'?>
<widget id="com.sector4f.pananda" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Pananda</name>
    <description>
        App to explore the National Historical Commission of the Philippines historical markers.
    </description>
    <author email="seav80@gmail.com" href="http://www.sector4f.com">
        Eugene Alvin Villar
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-screen-orientation" spec="^3.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="^2.4.1" />
    <preference name="StatusBarBackgroundColor" value="#112233" />
    <engine name="android" spec="~7.0.0" />
</widget>
